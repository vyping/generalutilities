<?xml version="1.0" encoding="UTF-8"?>

<selector
xmlns:android="http://schemas.android.com/apk/res/android">
<item
    android:state_pressed="true">
    <shape
        android:shape="rectangle">
        <stroke
            android:color="?colorButtonOutsidePressedMain"
            android:width="1dp"/>
        <gradient
            android:angle="90"
            android:startColor="?colorButtonOutsidePressedSecundary"
            android:centerColor="?colorButtonOutsidePressedMain"
            android:endColor="?colorButtonOutsidePressedSecundary" />
        <corners
            android:topLeftRadius="68dp"
            android:topRightRadius="24dp"
            android:bottomRightRadius="68dp"
            android:bottomLeftRadius="68dp"/>
    </shape>
</item>
<item>
    <shape
        android:shape="rectangle">
        <stroke
            android:color="?colorButtonOutsideMain"
            android:width="1dp"/>
        <gradient
            android:angle="90"
            android:startColor="?colorButtonOutsideSecundary"
            android:centerColor="?colorButtonOutsideMain"
            android:endColor="?colorButtonOutsideSecundary" />
        <corners
            android:topLeftRadius="68dp"
            android:topRightRadius="24dp"
            android:bottomRightRadius="68dp"
            android:bottomLeftRadius="68dp"/>
    </shape>
</item>
</selector>