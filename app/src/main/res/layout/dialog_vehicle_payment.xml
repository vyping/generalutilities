<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:mask="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_weight="1">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/ContainerTags"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlagr"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:elevation="8dp"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_tag"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextCard"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Id Tag"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_weight="0"
                android:layout_marginTop="8dp"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="10dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_register"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextVehicle"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Register"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_weight="0"
                android:layout_marginTop="8dp"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingEnd="16dp">

                <ImageView
                    android:id="@+id/iconTypeVehicle"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="10dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_car"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextTypeVehicle"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Type Vehicle"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_marginTop="24dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag67"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_enter"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextHourEnter"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Enter Hour"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag5"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_exit"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextHourOut"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Exit Hour"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag3"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_clock"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextHours"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Total Hours"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag27"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_money"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextPayment"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="Payment"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_marginTop="24dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag24"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_user"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextUser"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="User Name"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag246"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_idcard"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextDocument"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="80,123,456"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:layout_marginTop="8dp"
                android:layout_weight="0"
                android:background="@drawable/layout_rounded_white"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivFlag2"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/vehicles_shape_type"
                    android:padding="14dp"
                    android:scaleType="centerInside"
                    app:srcCompat="@drawable/icon_phone"
                    app:tint="@color/colorPrimaryDark10" />

                <TextView
                    android:id="@+id/TextPhone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="24dp"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="301 411 4569"
                    android:padding="8dp"
                    android:textColor="@color/colorNegro"
                    android:textColorHint="@color/GreyLight18"
                    android:textSize="22dp"
                    android:textStyle="bold" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</RelativeLayout>
