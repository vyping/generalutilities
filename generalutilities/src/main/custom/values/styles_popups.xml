<resources>

    <style name="PopupContainerLeft">
        <item name="android:layout_width">68dp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:background">@drawable/popup_container_left</item>
        <item name="android:backgroundTint">?colorPopUpsContainers</item>
        <item name="android:gravity">center_vertical|end</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="PopupContainerCenter">
        <item name="android:layout_width">52dp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:background">@drawable/popup_container_center</item>
        <item name="android:backgroundTint">?colorPopUpsContainers</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="PopupContainerCenterLong">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:background">@drawable/popup_container_center</item>
        <item name="android:backgroundTint">?colorPopUpsContainers</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="PopupContainerRight">
        <item name="android:layout_width">68dp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:layout_weight">0</item>
        <item name="android:background">@drawable/popup_container_right</item>
        <item name="android:backgroundTint">?colorPopUpsContainers</item>
        <item name="android:gravity">center_vertical|end</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="PopupButton">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@drawable/popup_button_short</item>
        <item name="android:backgroundTint">?colorPopUpsButtons</item>
        <item name="android:fontFamily">@font/solid</item>
        <item name="android:gravity">center</item>
        <item name="android:text">\uf03e</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">?colorPopUpsIcons</item>
        <item name="android:textSize">@dimen/fontPlus8</item>
    </style>

    <style name="PopupButtonLong">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@drawable/popup_internal_long</item>
        <item name="android:backgroundTint">?colorPopUpsButtons</item>
        <item name="android:fontFamily">@font/solid</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="android:text">Texto</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">?colorPopUpsIcons</item>
        <item name="android:textSize">@dimen/fontPlus6</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="PopupSpace">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">2</item>
    </style>

    <style name="PopUpTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">2</item>
    </style>

    <style name="PopUpListButtons">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">24sp</item>
    </style>

</resources>
