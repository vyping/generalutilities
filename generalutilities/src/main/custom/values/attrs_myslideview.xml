<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SlideData">
        <attr format="reference|color" name="slideBackground"/>
        <attr format="reference" name="slideSrc"/>
        <attr format="dimension" name="slideSrcMargin"/>
        <attr format="dimension" name="slideSrcMarginLeft"/>
        <attr format="dimension" name="slideSrcMarginRight"/>
        <attr format="dimension" name="slideSrcMarginTop"/>
        <attr format="dimension" name="slideSrcMarginBottom"/>
