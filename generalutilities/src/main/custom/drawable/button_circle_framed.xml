<?xml version="1.0" encoding="UTF-8"?>

<selector

    xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_pressed="true">
        <shape
            android:shape="oval">
            <stroke
                android:color="@color/GreyDark15"
                android:width="3dp" />
            <solid
                android:color="@color/GreyDark10" />
        </shape>
    </item>

    <item>
        <shape
            android:shape="oval">
            <stroke
                android:color="@color/colorNegro"
                android:width="3dp" />
            <solid
                android:color="@color/GreyDark15"/>
        </shape>
    </item>

</selector>
