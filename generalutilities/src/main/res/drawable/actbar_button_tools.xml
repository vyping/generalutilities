<selector
xmlns:android="http://schemas.android.com/apk/res/android">

<item
    android:state_pressed="true">
  <shape
      android:shape="rectangle">
    <stroke
        android:color="?colorActionBarSecundaryButtonsPressed"
        android:width="1dp"/>
    <solid android:color="?colorActionBarSecundaryButtonsPressed"/>
    <corners android:radius="10dp" />
  </shape>
</item>
<item>
  <shape
      android:shape="rectangle">
    <stroke
        android:color="?colorActionBarSecundaryButtons"
        android:width="1dp"/>
    <solid android:color="?colorActionBarSecundaryButtons"/>
    <corners android:radius="10dp" />
  </shape>
</item>
</selector>
